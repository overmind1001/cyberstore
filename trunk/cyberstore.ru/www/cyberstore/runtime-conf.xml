<?xml version="1.0" encoding="UTF-8"?>
<config>
  <!-- Uncomment this if you have PEAR Log installed
  <log>
    <type>file</type>
    <name>/path/to/propel.log</name>
    <ident>propel-bookstore</ident>
    <level>7</level>
  </log>
  -->
  <propel>
    <datasources default="cyberstore">
      <datasource id="cyberstore">
        <adapter>mysql</adapter> <!-- sqlite, mysql, mssql, oracle, or pgsql -->
        <connection>
          <dsn>mysql:host=localhost;dbname=db_cyberstore</dsn>
          <user>root</user>
          <password></password>
          <settings>
			  <setting id="charset">utf8</setting>
          </settings>
        </connection>
      </datasource>
    </datasources>
  </propel>
</config>